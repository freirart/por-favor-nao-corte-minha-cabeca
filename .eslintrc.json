{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["plugin:react/recommended", "plugin:prettier/recommended"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": "error",
    "testing-library/await-async-query": "off",
    "import/no-anonymous-default-export": "off"
  },
  "plugins": ["eslint-plugin-prettier", "react"]
}
